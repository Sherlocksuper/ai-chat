                    "404": {
                        "description": "Not Found",
                        "schema": {}
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {}
                    }
                }
            }
        },
        "/email/send": {
            "get": {
                "description": "send email",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "email"
                ],
                "summary": "send email",
                "parameters": [
                    {
                        "type": "string",
                        "description": "target email",
                        "name": "targetEmail",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
